<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sc.w_drill" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActNewDictionary"
            android:label="@string/title_activity_act_new_dictionary" >
        </activity>
        <activity
            android:name=".ActDictionaryList"
            android:label="@string/title_activity_act_dictionary_list" >
        </activity>
        <activity
            android:name=".ActDictionaryEntry"
            android:label="@string/title_activity_act_dictionary_entry" >
        </activity>
        <activity
            android:name=".ExportWordsActivity"
            android:label="@string/title_activity_export_wirds" >
        </activity>
    </application>

</manifest>
